<?xml version="1.0" encoding="utf-8"?>
<com.np.a08scroller.demo4.MultiTouchLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_multi_touch"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    tools:context="com.np.a08scroller.demo4.MultiTouchActivity">

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@mipmap/meinv4" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@mipmap/meinv" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@mipmap/meinv2" />
</com.np.a08scroller.demo4.MultiTouchLayout>

