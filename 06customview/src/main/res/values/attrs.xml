<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FirstView">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="AttrView">
        <attr name="attr1" format="string" />
        <attr name="attr2" format="string" />
        <attr name="attr3" format="string" />
        <attr name="attr4" format="string" />
    </declare-styleable>
    <attr name="myStyle" format="reference" />

    <declare-styleable name="CircleImageView">
        <attr name="circle_border" format="dimension" />
        <attr name="circle_border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CodeView">
        <attr name="code_text_size" format="dimension" />
        <attr name="code_text_color" format="color" />
        <attr name="code_count" format="integer" />
        <attr name="line_count" format="integer" />
    </declare-styleable>

    <declare-styleable name="WaterImageView">
        <attr name="water_image" format="reference" />
        <attr name="water_text" format="string" />
        <attr name="water_position" format="enum">
            <enum name="left_top" value="0" />
            <enum name="right_top" value="1" />
            <enum name="left_bottom" value="2" />
            <enum name="right_bottom" value="3" />
        </attr>
    </declare-styleable>
</resources>